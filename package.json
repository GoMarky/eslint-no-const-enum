{
  "name": "@gomarky/eslint-plugin-no-const-enum",
  "version": "0.0.4",
  "description": "ESLint rules that enum doesn't contains const keyword",
  "main": "./lib/index.js",
  "scripts": {
		"test": "node scripts/node-tests.js",
		"test:dist": "TEST_DIST=true npm test",
		"test:package": "TEST_PACKAGE=true npm test",
		"test:all": "npm test && npm run test:dist && npm run test:package",
		"coverage": "istanbul cover scripts/node-tests.js",
		"coverage:report": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build-core": "node scripts/build-core.js",
    "build": "npm run build-core",
    "prepare": "npm run build",
    "prepublish": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "qunit": "^2.10.1",
    "terser": "^4.6.12"
  },
  "peerDependencies": {
    "eslint": ">= 7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoMarky/eslint-no-const-enum.git"
  },
  "keywords": [
    "eslint",
    "plugin",
    "eslintplugin",
    "rule"
  ],
  "bugs": {
    "url": "https://github.com/GoMarky/eslint-no-const-enum/issues",
    "email": "swen295@gmail.com"
  },
  "homepage": "https://github.com/GoMarky/eslint-no-const-enum",
  "author": "Andrew Slesarenko <swen295@gmail.com>",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "test": "tests"
  }
}
